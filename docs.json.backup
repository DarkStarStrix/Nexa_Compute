    "prerequisites": [
      {
        "item": "Node.js >= 19.0.0",
        "verification": "node --version"
      },
      {
        "item": "npm >= 9.0.0",
        "verification": "npm --version"
      },
      {
        "item": "Mint CLI",
        "installation": "npm install -g mint"
      }
    ],
    "setupSteps": [
      {
        "step": 1,
        "title": "Install Mint tooling",
        "commands": [
          "npm install -g mint"
        ],
        "notes": "Use a Node version manager such as fnm or nvm if you need to match the Mint-supported Node release."
      },
      {
        "step": 2,
        "title": "Initialize Mint project files",
        "commands": [
          "cd /Users/allanmurimiwandia/Nexa_compute",
          "mint init --docs-dir docs"
        ],
        "notes": "If mint init asks to create example content, decline so the existing docs directory remains untouched."
      },
      {
        "step": 3,
        "title": "Configure navigation",
        "actions": [
          "Open docs.json at the repository root.",
          "Replace or update the navigation section with the navigation groups shown above.",
          "Update logo, social, or analytics fields if desired."
        ]
      },
      {
        "step": 4,
        "title": "Add frontmatter to Markdown files where needed",
        "notes": "Mint routes are derived from frontmatter. Add a title and slug (no leading slash) to each Markdown file, especially when nesting under subdirectories.",
        "exampleFrontmatter": "---\\ntitle: Start Here\\nslug: start-here\\ndescription: Day-one execution checklist for NexaCompute.\\n---"
      },
      {
        "step": 5,
        "title": "Verify internal links and assets",
        "actions": [
          "Move any images or attachments into docs/assets/ (create the directory if it does not exist).",
          "Rewrite relative links so they reference the Mint slug (for example, overview/architecture)."
        ]
      }
    ],
    "navigationTemplate": {
