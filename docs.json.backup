    "previewAndBuild": {
      "localPreview": {
        "commands": [
          "mint dev --docs-dir docs --port 3000"
        ],
        "notes": "Mint automatically watches the docs directory and reloads on file changes."
      },
      "contentValidation": {
        "commands": [
          "mint broken-links --docs-dir docs"
        ],
        "notes": "Run inside the repository root to surface missing slugs, invalid links, or formatting issues."
      },
      "staticExport": {
        "commands": [
          "mint build --docs-dir docs --output dist"
        ],
        "notes": "Creates a static bundle inside the dist/ directory that can be hosted on any static site provider."
      }
    },
    "deploymentOptions": [
      {
        "target": "Mint-hosted",
        "instructions": "Use mint deploy to push the site to Mint hosting. Requires linking the repository to a Mint project dashboard."
      },
      {
        "target": "Custom hosting",
        "instructions": "Serve the build output from dist/ via Netlify, Vercel, GitHub Pages, or a static bucket. Configure the target host to rewrite requests to index.html."
      }
    ],
    "contentNotes": [
      "Keep Markdown headings in sentence case so Mint automatically generates clean URLs when slugs are omitted.",
      "Replace inline HTML tables with Markdown tables when possible for consistent styling.",
      "Highlight code blocks with fenced syntax (```bash) so Mint applies syntax highlighting correctly.",
      "Add tags or metadata to docs.json if you plan to expose search facets."
    ],
    "qualityChecks": [
      {
        "item": "Broken links",
        "method": "Run mint broken-links and manually click through navigation during mint dev."
      },
      {
        "item": "SEO metadata",
        "method": "Add description and og metadata in docs.json or individual frontmatter entries."
      },
      {
        "item": "Asset load times",
        "method": "Optimize large diagrams before placing them in docs/assets/."
      }
    ]
