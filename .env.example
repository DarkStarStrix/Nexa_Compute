# Nexa Compute Environment Configuration

# API Keys
OPENROUTER_API_KEY=your_openrouter_key_here
HUGGINGFACE_TOKEN=your_hf_token_here
WANDB_API_KEY=your_wandb_key_here

# Cloud Storage - DigitalOcean Spaces (recommended)
DO_SPACES_KEY=your_do_spaces_key
DO_SPACES_SECRET=your_do_spaces_secret
DO_SPACES_BUCKET=nexa-artifacts
DO_SPACES_REGION=nyc3
DO_SPACES_ENDPOINT=https://nyc3.digitaloceanspaces.com

# Cloud Storage - AWS S3 (alternative)
# AWS_ACCESS_KEY_ID=your_aws_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret
# S3_BUCKET=nexa-artifacts
# S3_REGION=us-east-1

# GPU Providers
PRIME_INTELLECT_API_KEY=your_prime_intellect_key
PRIME_INTELLECT_API_URL=https://api.primeintellect.ai

# VPS Configuration
VPS_HOST=your_vps_ip_or_domain
VPS_API_URL=http://your_vps_ip:8000

# SSH Configuration
SSH_KEY_PATH=~/.ssh/id_rsa

# Repository
REPO_URL=https://github.com/DarkStarStrix/Nexa_Compute.git
